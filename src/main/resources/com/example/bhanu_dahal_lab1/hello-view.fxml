<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="468.0" prefWidth="621.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bhanu_dahal_lab1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <TableView fx:id="Order_History" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="OrderID" prefWidth="140.0" text="OrderID" />
       <TableColumn fx:id="UserName" prefWidth="96.0" text="UserName" />
         <TableColumn fx:id="Email" prefWidth="134.0" text="Email" />
         <TableColumn fx:id="Adress" prefWidth="172.0" text="Adress" />
     </columns>
   </TableView>
</VBox>
